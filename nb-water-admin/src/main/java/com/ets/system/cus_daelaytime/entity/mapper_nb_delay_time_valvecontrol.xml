<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ets.system.cus_daelaytime.dao.ValveControlDao">

	<!-- 更新 -->
    <update id="update" parameterType="com.ets.system.cus_daelaytime.entity.nb_delay_time_valvecontrol">
        update nb_delay_time_valvecontrol set value=#{value} where id=#{id}
    </update>
    
     <!-- 查看 -->
    <select id="info" resultType="com.ets.system.cus_daelaytime.entity.nb_delay_time_valvecontrol" parameterType="java.lang.String">
        select * from nb_delay_time_valvecontrol where customerId=#{customerId}
    </select>
    
    <!-- 新增 -->
    <insert id="add" parameterType="com.ets.system.cus_daelaytime.entity.nb_delay_time_valvecontrol">
        insert into nb_delay_time_valvecontrol values(#{id},#{value},#{customerId})
     </insert>
     
</mapper>