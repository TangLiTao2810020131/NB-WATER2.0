<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ets.business.daelaytime.dao.BasicNumDao">

     <!-- 根据客户ID获取设置表读数命令下发延迟时间 -->
    <select id="info" resultType="com.ets.business.daelaytime.entity.nb_delay_time_basicnum" parameterType="java.lang.String">
        select * from nb_delay_time_basicnum where customerId=#{customerId}
    </select>
     
</mapper>